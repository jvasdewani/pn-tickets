<%= page_title "New product type" %>

<div class="page_head">
  <h1>New Product Type</h1>
</div>

<div class="new person">
  <%= form_for @product do |f| %>
    <%= render :partial => 'form', :locals => { :f => f } %>
    <p class="button"><%= f.submit "Create product type", :disable_with => 'Saving changes' %> <%= link_to "Cancel", settings_path %></p>
  <% end %>
</div>
